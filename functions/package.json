{
  "name": "functions",
  "scripts": {
    "clean": "rimraf ./tmp && rimraf ./lib/**/*",
    "build:dev": "npm run clean && tsc --watch",
    "build": "npm run clean && tsc -p tsconfig-build.json && rollup -c rollup.config.js && rimraf ./tmp",
    "serve:dev": "concurrently \"npm run build:dev\" \"firebase emulators:start --inspect-functions\"",
    "serve": "npm run build ",
    "emulators:start": "firebase emulators:start",
    "shell": "firebase functions:shell",
    "start": "npm run build && npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "lint": "tslint -p tsconfig.json src/**/*.ts -e tests/**/*.ts",
    "test": "jest"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/firestore": "^4.2.0",
    "axios": "^0.21.1",
    "body-parser": "^1.18.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "2.8.5",
    "express": "^4.16.4",
    "firebase-admin": "9.1.0",
    "firebase-functions": "3.9.1",
    "moment": "^2.27.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@firebase/testing": "^0.20.11",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "4.2.12",
    "@types/cors": "2.8.7",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/mocha": "8.0.3",
    "@types/multer": "^1.4.4",
    "@types/rx": "^4.1.2",
    "@types/sinon": "9.0.4",
    "@types/sinon-chai": "3.2.4",
    "babel-jest": "^26.3.0",
    "chai": "4.2.0",
    "concurrently": "5.3.0",
    "eventemitter3": "^4.0.7",
    "faker": "^5.1.0",
    "firebase-functions-test": "^0.2.2",
    "firebase-mock": "^2.3.2",
    "jest": "^26.4.2",
    "mocha": "8.1.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "rimraf": "3.0.2",
    "rollup": "2.26.3",
    "rollup-plugin-node-resolve": "5.2.0",
    "sinon": "9.0.3",
    "sinon-chai": "3.5.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  },
  "private": true
}
