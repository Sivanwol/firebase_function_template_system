{
    "name": "functions",
    "scripts": {
        "copy:environment": "npm run clean && gulp compile:config",
        "copy:environment:develop": "rimraf .runtimeconfig.json && gulp compile:config --env develop",
        "environment": "test -f .runtimeconfig.json && firebase functions:config:unset env && firebase functions:config:set env=\"$(cat .runtimeconfig.json)\" || echo \"Please add the file .runtimeconfig.json before deploy.\"",
        "clean": "rimraf ./tmp && rimraf ./.tmp && rimraf ./lib/**/*",
        "build:dev": "npm run clean && tsc --watch",
        "build:debug": "tsc -p tsconfig-build.json && rollup -c rollup.config.js",
        "build": "npm run clean &&  npm run copy:environment:develop && npm run environment && tsc -p tsconfig-build.json && rollup -c rollup.config.js",
        "serve:dev:config": "firebase functions:config:get > .runtimeconfig.json",
        "serve:dev": "npm run copy:environment && concurrently \"npm run serve:dev:config && npm run build:dev\" \"firebase emulators:start --inspect-functions\"",
        "serve": "npm run build ",
        "emulators:start": "firebase emulators:start",
        "shell": "firebase functions:shell",
        "start": "npm run build && npm run shell",
        "deploy:dev": "firebase deploy --only functions",
        "logs": "firebase functions:log",
        "lint": "tslint -p tsconfig.json src/**/*.ts -e __tests__/**/*.ts",
        "test": "jest"
    },
    "main": "lib/index.js",
    "dependencies": {
        "@google-cloud/firestore": "^4.2.0",
        "@google-cloud/logging-winston": "^4.0.0",
        "axios": "^0.20.0",
        "body-parser": "^1.18.3",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "cors": "2.8.5",
        "express": "^4.16.4",
        "firebase-admin": "9.2.0",
        "firebase-functions": "^3.11.0",
        "ioredis": "^4.17.3",
        "lodash": "^4.17.20",
        "moment": "^2.27.0",
        "redis": "^3.0.2",
        "reflect-metadata": "^0.1.13",
        "routing-controllers": "^0.9.0-alpha.6",
        "rxjs": "^6.6.3",
        "simple-cached-firestore": "^3.2.9",
        "validated-base": "^1.0.2",
        "winston": "^3.3.3",
        "winston-logzio": "^5.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.11.4",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-typescript": "^7.10.4",
        "@firebase/testing": "^0.20.11",
        "@rollup/plugin-json": "^4.1.0",
        "@types/body-parser": "^1.19.0",
        "@types/cors": "2.8.7",
        "@types/express": "^4.17.7",
        "@types/faker": "^4.1.12",
        "@types/multer": "^1.4.4",
        "babel-jest": "^26.3.0",
        "chai": "4.2.0",
        "concurrently": "5.3.0",
        "eventemitter3": "^4.0.7",
        "faker": "^5.1.0",
        "firebase-functions-test": "^0.2.2",
        "firebase-mock": "^2.3.2",
        "firestore-seed": "^0.0.11",
        "gulp": "^4.0.2",
        "gulp-clean": "^0.4.0",
        "gulp-copy": "^4.0.1",
        "gulp-if": "^3.0.0",
        "gulp-param": "^1.0.3",
        "gulp-rename": "^2.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "gulp-yaml": "^2.0.4",
        "gulp-yaml-include": "^0.3.1",
        "gulp-yaml-merge": "^2.0.0",
        "jest": "^26.4.2",
        "nyc": "^15.1.0",
        "path-exists": "^4.0.0",
        "proxyquire": "^2.1.3",
        "rimraf": "3.0.2",
        "rollup": "2.26.3",
        "rollup-plugin-node-resolve": "5.2.0",
        "run-sequence": "^2.2.1",
        "ts-essentials": "^7.0.0",
        "ts-node": "8.10.2",
        "tslint": "6.1.3",
        "typescript": "3.9.7",
